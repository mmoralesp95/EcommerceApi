{
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_server;Database=your_database;User Id=your_user;Password=your_password;TrustServerCertificate=True;"
  },
  "Jwt": {
    "Key": "your_jwt_secret_key_here",
    "Issuer": "your_issuer_here",
    "Audience": "your_audience_here",
    "ExpiryInMinutes": 60
  },
  "Stripe": {
    "SecretKey": "your_stripe_secret_key_here",
    "WebhookSecret": "your_stripe_webhook_secret_here",
    "PublishableKey": "your_stripe_publishable_key_here"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
} 